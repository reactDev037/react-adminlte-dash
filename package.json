{
  "name": "react-adminlte-dash",
  "description": "React.js components for AdminLTE themed dashboard",
  "author": "Nate Greene <nate.greene@gmail.com>",
  "user": "zksailor534",
  "version": "0.0.7",
  "scripts": {
    "start": "webpack-dev-server",
    "test": "jest --",
    "test:coverage": "jest --coverage --",
    "test:watch": "jest --watch --",
    "test:lint": "eslint . --ext .js --ext .jsx --ignore-path .gitignore --ignore-pattern dist --cache",
    "gh-pages": "webpack",
    "gh-pages:deploy": "gh-pages -d gh-pages",
    "gh-pages:stats": "webpack --profile --json > stats.json",
    "dist": "webpack",
    "dist:min": "webpack",
    "dist:modules": "rm -rf ./dist-modules && babel ./src --out-dir ./dist-modules",
    "preversion": "npm run test && npm run dist && npm run dist:min",
    "prepublish": "npm run dist:modules",
    "postpublish": "npm run gh-pages && npm run gh-pages:deploy",
    "postinstall": "node lib/post_install.js"
  },
  "main": "dist-modules",
  "module": "src",
  "jsnext:main": "src",
  "devDependencies": {
    "admin-lte": "^2.3.8",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^17.0.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.11",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "chai": "^3.5.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "clean-webpack-plugin": "^0.1.14",
    "css-loader": "^0.26.1",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.7.0",
    "gh-pages": "^0.12.0",
    "git-prepush-hook": "^1.0.1",
    "html-webpack-plugin": "^2.24.1",
    "html-webpack-template": "^5.4.2",
    "jest": "^17.0.3",
    "json-loader": "^0.5.4",
    "raw-loader": "^0.5.1",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.1",
    "react-github-corner": "^0.3.0",
    "react-markdown": "^2.4.2",
    "react-router": "^3.0.0",
    "style-loader": "^0.13.1",
    "sync-exec": "^0.6.2",
    "system-bell-webpack-plugin": "^1.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^1.0.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react-bootstrap": "^0.30.7"
  },
  "peerDependencies": {
    "admin-lte": "^2.3.8",
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.7.0",
    "react": ">= 15.0.0 < 16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zksailor534/react-adminlte.git"
  },
  "homepage": "https://zksailor534.github.io/react-adminlte/",
  "bugs": {
    "url": "https://github.com/zksailor534/react-adminlte/issues"
  },
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "packages"
    ]
  },
  "keywords": [
    "react",
    "reactjs",
    "adminlte"
  ],
  "license": "MIT",
  "pre-push": [
    "test"
  ]
}
